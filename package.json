{
  "name": "code-snippets",
  "version": "1.0.0",
  "description": "代码片段整理",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "rm -rf dist .parcel-cache && parcel serve",
    "build": "rm -rf dist .parcel-cache && parcel build",
    "static:serve": "serve",
    "test": "jest",
    "zx:sync": "zx .scripts/sync.mjs"
  },
  "targets": {
    "frontend": {
      "source": [
        "./mp-platform/www/pages/index.html",
        "./mp-platform/www/pages/**/*.html",
        "./mp-platform/www/subpackages/**/*.html"
      ]
    }
  },
  "staticFiles": {
    "staticPath": "public",
    "staticOutPath": "public"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "keywords": [
    "snippet",
    "code snippet"
  ],
  "author": "lihzsky",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@jest/globals": "^29.7.0",
    "@parcel/config-default": "^2.14.4",
    "@parcel/packager-raw-url": "2.14.4",
    "@parcel/transformer-webmanifest": "2.14.4",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/benchmark": "^2.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "benchmark": "^2.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.14.4",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss": "^8.2.1",
    "serve": "^14.2.4",
    "tailwindcss": "^4.1.4",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "zx": "^8.5.2"
  },
  "dependencies": {
    "crc-32": "^1.2.2",
    "fast-png": "^6.3.0",
    "fflate": "^0.8.2",
    "uint8-to-base64": "^0.2.0"
  }
}