{
  "name": "octopus-svga",
  "version": "0.3.1",
  "keywords": ["svgaplayer", "svga"],
  "description": "高性能SVGA动效播放器，支持Web/微信小程序/支付宝小程序/抖音小程序，设计目标是 解析速度更快、体积更小、性能更高、兼容性更高、功能更丰富。",
  "type": "module",
  "module": "es/index.js",
  "main": "lib/index.js",
  "unpkg": "lib/index.min.js",
  "types": "index.d.ts",
  "exports": {
    "import": "./es/index.js",
    "default": "./lib/index.js",
    "unpkg": "./lib/index.min.js",
    "types": "./index.d.ts"
  },
  "files": [
    "es",
    "lib",
    "index.d.ts",
    "README.md"
  ],
  "scripts": {
    "declare": "tsc",
    "cleanup": "rimraf -rf es lib types index.d.ts",
    "generate": "rollup -c",
    "build": "npm run cleanup && npm run declare && npm run generate",
    "compiler": "pbjs -t static-module -w es6 -o protos/svga.js protos/svga.proto",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "lihzsky",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lihaizhong/CodeLabs.git"
  },
  "bugs": {
    "url": "https://github.com/lihaizhong/CodeLabs/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/jest": "^30.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "octopus-platform": "workspace:^"
  }
}
