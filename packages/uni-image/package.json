{
  "name": "uni-image",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "图片生成器",
  "module": "esm/index.js",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "exports": {
    "import": "./esm/index.js",
    "default": "./lib/index.js",
    "types": "./index.d.ts"
  },
  "files": [
    "esm",
    "lib",
    "index.d.ts",
